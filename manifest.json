{
    "name": "Elements",
    "description": "Highlight all elements on the page",
    "version": "0.3",
    "manifest_version": 3,
    "icons": {
        "128":"/icons/icon-128.png"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }],
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": ["activeTab", "scripting"]
    
}