{
  "name": "Elements",
  "description": "Highlight all elements on the page. It is a great tool for QA Engineers from Aleksandr Mironov",
  "version": "0.5",
  "manifest_version": 3,
  "icons": {
    "128": "/icons/icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": ["activeTab", "scripting"]
}
